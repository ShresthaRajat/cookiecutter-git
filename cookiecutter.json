{
  "git_name": "Rajat Shrestha",
  "git_email": "rajat.shrestha@eydean",
  "git_ignore": "windows,macos,linux,git",

  "repo_slug": "cookiecutter-git-demo",
  "repo_tagline": "A cookiecutter-git demonstration :tada:",
