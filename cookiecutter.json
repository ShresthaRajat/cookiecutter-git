  "remote_protocol": ["https", "ssh"],

  "code_of_conduct": ["contributor_covenant", "citizen_code_of_conduct"],
  "copyright_license": [
    "MIT",
    "Apache-2.0",
    "BSD-2-Clause",
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0",
    "AGPL-3.0",
    "LGPL-2.1",
    "LGPL-3.0",
    "EPL-1.0",
    "MPL-2.0",
    "Unlicense"
  ],
  "copyright_holder": "{{ cookiecutter.git_name }}",

  "make_dirs": "docs,src,tests",

  "_testing": false
}
